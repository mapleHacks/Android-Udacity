        {
          "id": 590373,
          "key": "7933128b-2a9f-4713-921b-3b39730c4488",
          "title": "Quiz: I hereby promise…",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "7933128b-2a9f-4713-921b-3b39730c4488",
            "completed_at": null,
            "last_viewed_at": null,
            "unstructured": null
          },
          "resources": null,
          "atoms": [
            {
              "id": 594939,
              "key": "557b9056-077a-45e3-95e8-8c865f866c94",
              "title": null,
              "semantic_type": "TextAtom",
              "is_public": true,
              "text": "Accessing the database in the main thread can be time consuming, and could lock the UI and throw an [Application Not Responding](https://developer.android.com/training/articles/perf-anr.html) error. To avoid that, Room will, by default, throw an error if you attempt to access the database in the main thread.\n\nIn the finished app, we need to implement the database operations to run asynchronously, but we also need to validate that what we have done so far is working, so let’s temporarily enable the option to allow queries in the main thread.",
              "instructor_notes": ""
            },
            {
              "id": 594940,
              "key": "2d8a6438-831f-45cf-857c-01f2d8d18685",
              "title": "Before going ahead",
              "semantic_type": "CheckboxQuizAtom",
              "is_public": true,
              "instructor_notes": null,
              "user_state": {
                "node_key": "2d8a6438-831f-45cf-857c-01f2d8d18685",
                "completed_at": null,
                "last_viewed_at": null,
                "unstructured": null
              },
              "question": {
                "prompt": "I hereby promise that...",
                "answers": [
                  {
                    "id": "a1523540376052",
                    "text": "I will not run queries in the main thread in production. I understand that this is just enabled temporarily to verify our implementation. I guarantee that I will disable this option immediately after this test, and I will implement the queries asynchronously.",
                    "is_correct": true
                  }
                ]
              }
            }
          ]
        },
