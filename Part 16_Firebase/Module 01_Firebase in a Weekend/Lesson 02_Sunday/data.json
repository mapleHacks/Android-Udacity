              "semantic_type": "ImageAtom",
              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2016/October/58124afc_storagediagram/storagediagram.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/a43a87b8-05df-4865-a469-8d4fdbb4a852",
              "caption": "",
